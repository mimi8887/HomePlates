<h2><%= @category_name %> Recipes</h2>
<ul>
  <% @recipes.each do |recipe| %>
    <li>
      <h2><%= link_to recipe["recipeTitle"], recipe_path(recipe["recipeId"], recipe: recipe.to_json) %></h2>
      <p><%= recipe["recipeDescription"] %></p>
    </li>
  <% end %>
</ul>
<%= link_to "Back to Top Recipes", root_path %>
